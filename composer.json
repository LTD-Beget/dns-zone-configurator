{
  "name": "ltd-beget/dns-zone-configurator",
  "license": "MIT",
  "type": "library",
  "description": "Php library for parsing and editing dns zones files programmatically with high level abstraction.",
  "keywords": [
    "dns",
    "zone file"
  ],
  "authors": [
    {
      "name": "Viskov Sergey",
      "homepage": "https://github.com/voksiv"
    }
  ],
  "require": {
    "php": ">=8.0",
    "marc-mabe/php-enum" : ">=2.2",
    "laminas/laminas-validator" : "^2.30.1",
    "ltd-beget/dns-zone-tokenizer": "~0.0.10"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.5.33",
    "tideways/profiler": "~2.0"
  },
  "autoload": {
    "psr-0": {
      "LTDBeget\\dns": "src"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "LTDBeget\\dev": "src"
    }
  }
}
